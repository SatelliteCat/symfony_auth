{% extends 'base.html.twig' %}

{% block title %}Registration
{% endblock %}

{% block body %}

	<div class="row">
		<div class="col-4 offset-4">
			<h1 class="h3 mb-3 font-weight-normal">Registration</h1>

			{{ form_start(registrationForm) }}
			<div class="form-group">
				{{ form_row(registrationForm.first_name) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.last_name) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.phone) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.user) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.organization) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.password) }}
			</div>
			<div class="form-group">
				{{ form_row(registrationForm.save) }}
			</div>

			{{ form_end(registrationForm) }}

		</div>
	</div>
{% endblock %}
